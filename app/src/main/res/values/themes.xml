<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.DuAn1_PRO1121" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="edt_style">
        <item name="android:textSize">18sp</item>
        <item name="android:background">@drawable/edt_background</item>
        <item name="android:paddingStart">8dp</item>
        <item name="android:textColorHint">@color/edt_color</item>
    </style>

    <style name="tablayout_style">
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="btn_style">
        <item name="android:textAllCaps">false</item>
        <item name="drawableTint">@color/white</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="item_text_style">
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="circle_img">
        <item name="cornerSize">50%</item>
    </style>

    <style name="corner_img_20">
        <item name="cornerSize">25%</item>
    </style>

    <style name="textActive">
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="textInActive">
        <item name="android:textSize">11sp</item>
    </style>
</resources>